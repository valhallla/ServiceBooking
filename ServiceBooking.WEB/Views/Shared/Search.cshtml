<br/>
@using (Html.BeginForm("Search", "Orders", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="text" id="name" class="search-control" name="name" data-autocomplete-source='@Url.Action("AutocompleteSearch", "Orders")'/>
    <input type="submit" value="Search" class="btn btn-default"/>
}

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
}

<script src='@Url.Content("~/Scripts/jquery-1.10.2.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Scripts/jquery-ui-1.11.3.min.js")' type="text/javascript"></script>

<script type="text/javascript">
    $(function() {
        $("[data-autocomplete-source]").each(function() {
            var target = $(this);
            target.autocomplete({ source: target.attr("data-autocomplete-source") });
        });
    });
</script>

